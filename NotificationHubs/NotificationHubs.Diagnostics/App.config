<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="NotificationHubName" value="hubname"/>
  </appSettings>
  <connectionStrings>
    <add name="NotificationHubs" connectionString="Endpoint=sb://[namespace].servicebus.windows.net/;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=[sharedaccesskey]"/>
  </connectionStrings>
</configuration>